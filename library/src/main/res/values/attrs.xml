<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TagSphereView">
        <attr name="rotateOnTouch" format="boolean" />
        <attr name="touchSensitivity" format="integer" />
        <attr name="radius" format="float" />
        <attr name="easingFunction" format="enum">
            <enum name="easeIn" value="0" />
            <enum name="easeOut" value="1" />
            <enum name="easeInExpo" value="2" />
            <enum name="easeOutExpo" value="3" />
            <enum name="inQuint" value="4" />
            <enum name="outQuint" value="5" />
            <enum name="none" value="6" />
        </attr>
    </declare-styleable>
</resources>